#= require eastwood/utils

###*
 * eastwood.js - <%= app %>
###
app = <%= target %>.<%= app %> ||= { }

<% I18n.backend.send :init_translations %>

app.I18n = <%= I18n.backend.send( :translations ).to_json %>

app.I18n.locale = "<%= I18n.locale %>"
app.I18n.t = ( key ) -> app.Utils.keypath key, @[ @locale ]
