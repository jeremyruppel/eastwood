###*
 * eastwood.js - <%= app %>
###
app = { env : "<%= env %>", routes : { } }

###*
 * Routes
###
<% routes.each do |key, route| %>
app.routes.<%= route.coffee_name %> = ( <%= route.coffee_args %> ) ->
  "<%= route.coffee_path %>"
<% end %>

###*
 * Hashes
###
<% hashes.each do |key, route| %>
app.routes.<%= route.coffee_name %> = ( <%= route.coffee_args %> ) ->
  "<%= route.coffee_path %>"
<% end %>

###*
 * Export
###
@window ||= { } # For ExecJS
@window.<%= app %> = app
