###*
 * eastwood.js - <%= app %>
###
app = { env : "<%= env %>", exports : { }, routes : { } }

###*
 * Exports
###

###*
 * Routes
###
<% routes.each do |key, route| %>
app.routes.<%= route.coffee_name %> = ( <%= route.coffee_args %> ) ->
  "<%= route.coffee_path %>"
<% end %>

###*
 * Hashes
###
<% hashes.each do |key, route| %>
app.routes.<%= route.coffee_name %> = ( <%= route.coffee_args %> ) ->
  "<%= route.coffee_path %>"
<% end %>

###*
 * Export
###
<%= target %>.<%= app %> = app
